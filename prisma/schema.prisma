generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite" 
  url      = "file:./data.db"
}

model Unit {
  id       Int     @id @default(autoincrement())
  details    String
} 

model Fowmap {
  id       Int     @id @default(autoincrement())
  details    String
  name  String
} 